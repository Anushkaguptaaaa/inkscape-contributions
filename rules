#!/usr/bin/make -f
# -*- makefile -*-
# debian/rules  for inkscape derived from a sample 
# provided by Joey Hess via dh_make.

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@ --with bash-completion,python2 --parallel

override_dh_auto_configure:
	export NOCONFIGURE=true && ./autogen.sh
	dh_auto_configure -- --with-gnome-vfs --enable-lcms
